{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Color picker",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "color-picker",
                "--video-path",
                "test.mp4",
                "--colors-path",
                "colors.yaml",
                "--frame-end",
                "250"

            ]
        },
        {
            "name": "Key picker",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "key-picker",
                "--video-path",
                "test.mp4",
                "--key-segments-path",
                "keys.yaml",

            ]
        },
        {
            "name": "Video to midi",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "video-to-midi",
                "--video-path",
                "test.mp4",
                "--key-segments-path",
                "keys.yaml",
                "--colors-path",
                "colors.yaml",
                "--midi-path",
                "output.midi",
                // "--frame-start",
                // "150",
                // "--frame-end",
                // "300",
            ]
        },
    ]
}
